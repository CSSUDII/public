{
    "name": "public",
    "version": "1.0.0",
    "main": "lib/index.js",
    "repository": "https://github.com/CSSUDII/public.git",
    "author": "lucaslah",
    "license": "MIT",
    "scripts": {
        "build": "babel src --out-dir lib --copy-files --extensions \".ts,.tsx\" --source-maps inline",
        "start": "node lib/index.js",
        "test": "jest",
        "commit": "node scripts/commit.git.scripts.js",
        "push": "node scripts/push.git.scripts.js",
        "cleanup": "rm -r -f lib",
        "browserify": "browserify ./lib/index.js -t babelify --outfile bundle.js",
        "ci": "yarn —-ignore-engines --frozen-lockfile",
        "retire": "retire",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --noEmit --watch",
        "build:types": "tsc --emitDeclarationOnly",
        "build:all": "yarn build:types && yarn build",
        "lint": "eslint . --ext .ts",
        "i": "yarn add —-ignore-engines"
    },
    "dependencies": {
        "@types/qrcode-svg": "^1.1.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "convert-svg-to-png": "^0.5.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.3.0",
        "helmet": "^4.6.0",
        "hsts": "^2.2.0",
        "inklog.js": "^1.2.1",
        "jimp": "^0.16.1",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.13.0",
        "morgan": "^1.10.0",
        "qrcode-svg": "^1.1.0",
        "react-bootstrap": "^1.6.1",
        "regenerator-runtime": "^0.13.7",
        "sharp": "^0.28.3"
    },
    "jest": {
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        }
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/preset-env": "^7.14.4",
        "@babel/preset-typescript": "^7.13.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.11",
        "@types/express": "^4.17.13",
        "@types/express-rate-limit": "^5.1.3",
        "@types/js-yaml": "^4.0.1",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/morgan": "^1.9.3",
        "@types/regenerator-runtime": "^0.13.0",
        "@types/sharp": "^0.28.3",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.1",
        "axios": "^0.21.1",
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "eslint": "^7.29.0",
        "retire": "^3.0.0",
        "supertest": "^6.1.3",
        "typescript": "^4.3.5"
    }
}
