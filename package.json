{
    "name": "public",
    "version": "1.0.0",
    "main": "lib/index.js",
    "repository": "https://github.com/CSSUDII/public.git",
    "author": "lucaslah <lucas.petrino@spycampllc.ml>",
    "license": "MIT",
    "scripts": {
        "build": "babel src --out-dir lib --copy-files --extensions \".ts,.tsx\" --source-maps inline",
        "start": "node lib/index.js",
        "test": "jest",
        "commit": "node scripts/commit.git.scripts.js",
        "push": "node scripts/push.git.scripts.js",
        "cleanup": "rm -r -f lib",
        "browserify": "browserify ./lib/index.js -t babelify --outfile bundle.js",
        "ci": "yarn install --frozen-lockfile",
        "retire": "retire",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --noEmit --watch",
        "build:types": "tsc --emitDeclarationOnly",
        "build:all": "yarn build:types && yarn build",
        "lint": "eslint . --ext .ts"
    },
    "dependencies": {
        "babel-jest": "27.0.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.6",
        "helmet": "^4.6.0",
        "hsts": "^2.2.0",
        "inklog.js": "^1.1.2",
        "jimp": "^0.16.1",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.12.11",
        "morgan": "^1.10.0",
        "regenerator-runtime": "^0.13.7"
    },
    "jest": {
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        }
    },
    "devDependencies": {
        "@babel/cli": "^7.14.3",
        "@babel/core": "^7.14.0",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.14.2",
        "@babel/preset-typescript": "^7.13.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.12",
        "@types/express-rate-limit": "^5.1.1",
        "@types/js-yaml": "^4.0.1",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/morgan": "^1.9.2",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "axios": "^0.21.1",
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "eslint": "^7.27.0",
        "jest": "26.6.3",
        "retire": "^3.0.0",
        "supertest": "^6.1.3",
        "typescript": "^4.3.2"
    }
}
